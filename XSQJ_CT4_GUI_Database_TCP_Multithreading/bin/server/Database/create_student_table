create database if not exists XSQJ;
use XSQJ;

CREATE TABLE studentlist
(
   
   
   username      VARCHAR(10)  not null,
   pwd       	 VARCHAR(10) not null,
   name   		 VARCHAR(10),
   stuNum    	 VARCHAR(9) unique,
   sex		 	 VARCHAR(1) ,  
   major		 VARCHAR(10),
   reason		 TEXT,
  state 		 VARCHAR(5)  not null,
    PRIMARY KEY (username)
    
) DEFAULT CHARSET=utf8;

